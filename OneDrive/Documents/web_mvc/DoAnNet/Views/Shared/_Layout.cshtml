    <link rel="stylesheet" href="node_modules/glidejs/glide/dist/css/glide.theme.min.css">
    <!--CSS-->
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/footer.css" rel="stylesheet" />
</head>
<body>

    <!--=====================================================-->
    <!--HEADER-->
    <!--=====================================================-->
    <header class="header" id="header">
        <!--Navigation-->
        <div class="navigation">
            <div class="container">
                <nav class="nav">
                    <div class="nav__hamburger">
                           <button class="btn dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                               <svg class="scroll-link" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"><path d="M0 96C0 78.3 14.3 64 32 64H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32C14.3 128 0 113.7 0 96zM0 256c0-17.7 14.3-32 32-32H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32c-17.7 0-32-14.3-32-32zM448 416c0 17.7-14.3 32-32 32H32c-17.7 0-32-14.3-32-32s14.3-32 32-32H416c17.7 0 32 14.3 32 32z" /></svg> 
                           </button>
                    </div>

                    <div class="nav nav__logo">
                        <a href="/Products/showProduct" class="scroll-link">PHONE</a>
                    </div>

                    <div class="nav__menu">
                        <div class="menu__top">
                            <span class="nav__category">PHONE</span>
                            <a href="/Products/showProduct" class="close__toggle">
                                <svg xmlns="http://www.w3.org/2000/svg" height="2rem" viewBox="0 0 384 512"><path d="M342.6 150.6c12.5-12.5 12.5-32.8 0-45.3s-32.8-12.5-45.3 0L192 210.7 86.6 105.4c-12.5-12.5-32.8-12.5-45.3 0s-12.5 32.8 0 45.3L146.7 256 41.4 361.4c-12.5 12.5-12.5 32.8 0 45.3s32.8 12.5 45.3 0L192 301.3 297.4 406.6c12.5 12.5 32.8 12.5 45.3 0s12.5-32.8 0-45.3L237.3 256 342.6 150.6z" /></svg>
                            </a>
                        </div>

                        <ul class="nav__list">
                            <li class="nav__item">
                                <a href="/Products/showProduct" class="nav__link scroll-link">Home</a>
                            </li>

                            <li class="nav__item">
                                <div class="dropdown">
                                    <button style="font-size : 14px ; color : #0D6EFD" class="btn dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                                      CATEGORY
                                    </button>
                                    <ul class="dropdown-menu">
                                        <li style="padding-left : 10px"><a href="/Products/Apple" class="scroll-link" style="font-size : 14px">APPLE</a></li>
                                        <hr />
                                        <li style="padding-left : 10px"><a href="/Products/SamSung" class="scroll-link" style="font-size : 14px">SAMSUNG</a></li>
                                        <hr />
                                        <li style="padding-left : 10px""><a href="/Products/HeadPhone" class="scroll-link" style="font-size : 14px">HEADPHONE</a></li>
                                    </ul>
                                </div>
                            </li>

                            <li class="nav__item">
                                <a href="/Contacts/Create" class="nav__link scroll-link">Contact</a>
                            </li>

                            <li>
                                <div class="topnav" style="height">
                                    <form method="get" action="/Products/Search">
                                        <input style="height:30px ; width:200px" type="text" placeholder="Search.." ; name="txt_search">                                           
                                        </form>
                                </div>
                            </li>

                            @{ var user = Session["TaiKhoan"] as DoAnNet.Models.TaiKhoan;}
                            @if (Session["TaiKhoan"] != null)
                            {

                            }
                        </ul>

                    </div>

                    <!-------User-->
                    <div class="nav__icons">
                        @if (Session["TaiKhoan"] == null)
                        {
                            <a href="/TaiKhoans/login" class="icon__item">
                                <svg xmlns="http://www.w3.org/2000/svg" height="2rem" viewBox="0 0 448 512"><path d="M304 128a80 80 0 1 0 -160 0 80 80 0 1 0 160 0zM96 128a128 128 0 1 1 256 0A128 128 0 1 1 96 128zM49.3 464H398.7c-8.9-63.3-63.3-112-129-112H178.3c-65.7 0-120.1 48.7-129 112zM0 482.3C0 383.8 79.8 304 178.3 304h91.4C368.2 304 448 383.8 448 482.3c0 16.4-13.3 29.7-29.7 29.7H29.7C13.3 512 0 498.7 0 482.3z" /></svg>
                            </a>
                        }
                        else
                        {
                            <a href="/TaiKhoans/UserProfile" class="icon__item">
                                <svg xmlns="http://www.w3.org/2000/svg" height="2rem" viewBox="0 0 448 512"><path d="M304 128a80 80 0 1 0 -160 0 80 80 0 1 0 160 0zM96 128a128 128 0 1 1 256 0A128 128 0 1 1 96 128zM49.3 464H398.7c-8.9-63.3-63.3-112-129-112H178.3c-65.7 0-120.1 48.7-129 112zM0 482.3C0 383.8 79.8 304 178.3 304h91.4C368.2 304 448 383.8 448 482.3c0 16.4-13.3 29.7-29.7 29.7H29.7C13.3 512 0 498.7 0 482.3z" /></svg>
                            </a>
                        }

                        <!--Giỏ hàng-->
                        <a href="/ShoppingCartItems/Index" class="icon__item">
                            <svg xmlns="http://www.w3.org/2000/svg" height="2rem" viewBox="0 0 576 512"><!--! Font Awesome Free 6.4.2 by - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M0 24C0 10.7 10.7 0 24 0H69.5c22 0 41.5 12.8 50.6 32h411c26.3 0 45.5 25 38.6 50.4l-41 152.3c-8.5 31.4-37 53.3-69.5 53.3H170.7l5.4 28.5c2.2 11.3 12.1 19.5 23.6 19.5H488c13.3 0 24 10.7 24 24s-10.7 24-24 24H199.7c-34.6 0-64.3-24.6-70.7-58.5L77.4 54.5c-.7-3.8-4-6.5-7.9-6.5H24C10.7 48 0 37.3 0 24zM128 464a48 48 0 1 1 96 0 48 48 0 1 1 -96 0zm336-48a48 48 0 1 1 0 96 48 48 0 1 1 0-96z" /></svg>
                            <span id="cart__total">0</span>
                        </a>
</div>
                </nav>
            </div>
        </div>
        <!--Slide Show-->
        <div class="slideshow">
            <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
                <div class="carousel-indicators">
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
                </div>
                <div class="carousel-inner">
                    <div class="carousel-item active ">
                        <div class="hero__center">
                            <div class="hero__left">
                                <span>New Inspirarion 2023</span>
                                <h1>PHONE MADE FOR YOU!</h1>
                                <p>Trending from mobile and headphone style collection</p>
                                <a href="#"><button class="hero__btn">SHOP NOW</button></a>
                            </div>
                            <div class="hero__right">
                                <div class="hero__img-container">
                                    <img src="~/Image/vn_iphone_15_pink_pdp_image_position-3_design_detail_2.jpg" />
                                </div>
                            </div>
                        </div>
                        <div class="carousel-caption d-none d-md-block">
                        </div>
                    </div>
                    <div class="carousel-item">

                        <div class="hero__center">
                            <div class="hero__left">
                                <span>New Inspirarion 2023</span>
                                <h1>PHONE MADE FOR YOU!</h1>
                                <p>Trending from mobile and headphone style collection</p>
                                <a href="#"><button class="hero__btn">SHOP NOW</button></a>
                            </div>
                            <div class="hero__right">
                                <div class="hero__img-container">
                                    <img src="~/Image/iphone15_pink.jpg" />
                                </div>
                            </div>
                        </div>
                        <div class="carousel-caption d-none d-md-block">
                        </div>
                    </div>
                    <div class="carousel-item">
                        <div class="hero__center">
                            <div class="hero__left">
                                <span>New Inspirarion 2023</span>
                                <h1>PHONE MADE FOR YOU!</h1>
                                <p>Trending from mobile and headphone style collection</p>
                                <a href="#"><button class="hero__btn">SHOP NOW</button></a>
                            </div>
                            <div class="hero__right">
                                <div class="hero__img-container">
                                    <img src="~/Image/panner.jpg" />
                                </div>
                            </div>
                        </div>
                        <div class="carousel-caption d-none d-md-block">
                        </div>
                    </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                    <span class="carousel-control-prev-icon btn btn-dark" aria-hidden="true"></span>
                    <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                    <span class="carousel-control-next-icon btn btn-dark" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                </button>
            </div>
        </div>

    </header>
    <!---Main-->
    <main id="main">
        <!--Colection-->
        <section class="section collection" id="" collection>
            <div class="container">
                <div class="collection__container">
                    <div class="collection__box">
                        <div class="img__container">
                            <img src="~/Image/airpods-iphone-7-apple-macbook-headphones-airpod.png" />
                        </div>
                        <div class="collection__content">
                            <div class="collection__data">
                                <span>New Colors Introduced</span>
                                <h1>HEADPHONES</h1>
                                <a href="#">SHOP NOW</a>
                            </div>
                        </div>
                    </div>

                    <div class="collection__box">
                        <div class="img__container">
                            <img src="~/Image/iphone15.png" />
                        </div>
                        <div class="collection__content">
                            <div class="collection__data">
                                <span>Phone Device Preset</span>
                                <h1>SMARTPHONES</h1>
                                <a href="#">SHOP NOW</a>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </section>
    </main>
</body>
</html>

